                                                    <p>Coupon Redeemed</p>
                                                    <% } else { %>
                                                        <div class="mt-4"><small>With Code : <%= coupon[i].code %></small></div>
                                                <button class="copy-coupon-btn btn btn-outline-2 " data-coupon="<%= coupon[i].code %>">

                                                    <span class="" style="font-size: medium;"><i class="fas fa-copy" ></i> Copy</span>
                                                </button>
                                                <% } %>
                                            </div>
                                        </div>
                                        <!-- End .card-dashboard -->
                                    </div><br><br>
                                    <!-- End .col-lg-6 -->
                            
                                    <% } } else { %>
